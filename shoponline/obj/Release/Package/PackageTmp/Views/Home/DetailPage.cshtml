@model ShopOnlineModel.EF.tb_Contents

<div class="breadcrumb">
    @Html.Raw(ViewBag.breadcrumb)
</div>
<div class="row">
    <div class="col-md-3">
        @Html.Action("MenuProduct")
    </div>
    <div class="col-md-9">
        <div class="page-title">
            <h1>
                @Model.Title
            </h1>
        </div>
        <div class="page-body">
            @Html.Raw(Model.Content)
        </div>
    </div>
</div>
