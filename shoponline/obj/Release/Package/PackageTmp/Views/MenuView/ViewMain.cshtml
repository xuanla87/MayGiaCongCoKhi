@model IEnumerable<ShopOnlineModel.EF.tb_Menus>

@{
    if (Model.Count() > 0)
    {
        <ul class="nav navbar-nav">
            @foreach (var item in Model)
            {
                <li>
                    <a href="@item.Url" title="@item.Text">
                        @item.Text
                    </a>
                </li>
            }
        </ul>
    }
}
